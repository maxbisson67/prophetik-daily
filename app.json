{
  "expo": {
    "owner": "maxbisson",
    "name": "Prophetik",
    "slug": "prophetik-daily",
    "scheme": "prophetik-daily",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/prophetik_red_1024.png",
    "backgroundColor": "#ffffff",
    "userInterfaceStyle": "light",
    "notification": {
      "icon": "./assets/notification-icon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "16.0",
            "swiftVersion": "5.0"
          }
        }
      ],
      "expo-router",
      "expo-notifications",
      "expo-font"
    ],
    "extra": {
      "firebase": {
        "apiKey": "AIzaSyCISpoKSI-2U7NB4AvRlKXTzdEmwzc7pKE",
        "authDomain": "capitaine.firebaseapp.com",
        "projectId": "capitaine",
        "messagingSenderId": "502697214487"
      },
      "router": {},
      "eas": {
        "projectId": "b58db99d-83c8-4770-a5ff-e940f63c5883"
      }
    },
    "ios": {
      "icon": "./assets/prophetik_red_1024.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.prophetik",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/prophetik_adaptive_foreground_432_transparent.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.prophetik",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "POST_NOTIFICATIONS"
      ],
      "softwareKeyboardLayoutMode": "resize"
    },
    "web": {
      "favicon": "./assets/prophetik_red_192.png"
    }
  }
}
