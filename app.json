{
  "expo": {
    "owner": "maxbisson",
    "name": "Prophetik",
    "slug": "prophetik-daily",
    "scheme": "prophetik-daily",
    "version": "3.0.2",
    "orientation": "portrait",
    "icon": "./assets/prophetik_icon_1024.png",
    "backgroundColor": "#ffffff",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/prophetik_icon_1024.png",
      "backgroundColor": "#FFFFFF",
      "resizeMode": "contain"
    },
    "updates": {
      "checkAutomatically": "ON_ERROR_RECOVERY",
      "fallbackToCacheTimeout": 0
    },
    "notification": {
      "icon": "./assets/notification-icon.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      "expo-router",
      "expo-notifications",
      "expo-font"
    ],
    "extra": {
      "firebase": {
        "apiKey": "AIzaSyCISpoKSI-2U7NB4AvRlKXTzdEmwzc7pKE",
        "authDomain": "capitaine.firebaseapp.com",
        "projectId": "capitaine",
        "messagingSenderId": "502697214487"
      },
      "router": {},
      "eas": {
        "projectId": "b58db99d-83c8-4770-a5ff-e940f63c5883"
      }
    },
    "ios": {
      "icon": "./assets/prophetik_icon_1024.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.prophetik",
      "buildNumber": "2.0.0",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "versionCode": 5,
      "backgroundColor": "#0B1020",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive_foreground_432_transparent_padded.png",
        "backgroundColor": "#E11D48"
      },
      "package": "com.prophetik",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "POST_NOTIFICATIONS"
      ],
      "softwareKeyboardLayoutMode": "resize"
    },
    "web": {
      "favicon": "./assets/prophetik_red_192.png"
    }
  }
}
